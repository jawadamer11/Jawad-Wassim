<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/k"
    tools:context=".A2_Player">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/profile"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:tint="@color/white"
        android:layout_marginTop="64dp"
        app:backgroundTint="#FFFFFF"
        android:clickable="true"
        android:contentDescription="@string/todo222"
        android:foreground="@drawable/ic_baseline_person_24"
        android:focusable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ImageContrastCheck,DuplicateSpeakableTextCheck"
        tools:srcCompat="@tools:sample/avatars" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/reg"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="24dp"
        app:backgroundTint="@color/white"
        android:clickable="true"
        android:contentDescription="@string/todo223"
        android:tint="@color/white"
        android:foreground="@drawable/ic_baseline_app_registration_24"
        android:focusable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:ignore="ImageContrastCheck"
        tools:srcCompat="@tools:sample/avatars" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/sched"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="16dp"
        android:clickable="true"
        android:contentDescription="@string/todo224"
        android:foreground="@drawable/cal"
        android:tint="@color/white"
        app:backgroundTint="@color/white"
        android:focusable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        tools:ignore="ImageContrastCheck"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="100dp"
        android:layout_height="26dp"
        android:gravity="center"
        android:layout_marginTop="8dp"
        android:textColor="@color/white"
        android:text="@string/profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="150dp"
        android:textColor="@color/white"
        android:layout_height="56dp"
        android:gravity="center"
        android:layout_marginTop="12dp"
        android:text="@string/register_renew_registration"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reg" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="150dp"
        android:layout_height="56dp"
        android:gravity="center"
        android:text="@string/make_your_schedule"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/sched"
        app:layout_constraintVertical_bias="0.048" />

    <ImageView
        android:id="@+id/logoutIMG"
        android:layout_width="32dp"
        android:layout_height="54dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.957"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.014"
        app:srcCompat="@mipmap/logout" />

</android.support.constraint.ConstraintLayout>